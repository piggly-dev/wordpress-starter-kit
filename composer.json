        "plugins",
        "wordpress development",
        "plugins development"
    ],
    "type": "library",
    "homepage": "https://github.com/piggly-dev/wordpress-starter-kit",
    "autoload": {
        "psr-4": {
            "Piggly\\Wordpress\\": "src"
